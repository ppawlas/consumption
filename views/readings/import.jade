extends ../layout

block content
	div(class='page-header')
		h1= title
		
	div(class='lpadForm')
		form(class='form-inline', role='form', method='POST', action=controllerPath + '/import', enctype='multipart/form-data')
			div(class='form-group')
				label(class='sr-only', for='import') File input
				input(type='file', id='import', name='import', multiple='multiple')
			button(type='submit', class='btn btn-default') Import